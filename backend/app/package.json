{
  "name": "saas-agendamiento-backend",
  "version": "1.0.0",
  "description": "Backend API para sistema SaaS de agendamiento multi-tenant",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "NODE_ENV=test jest --runInBand --forceExit",
    "test:clean": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage --forceExit",
    "test:e2e": "NODE_ENV=test jest __tests__/e2e --forceExit",
    "test:endpoints": "NODE_ENV=test jest __tests__/endpoints --forceExit",
    "test:integration": "NODE_ENV=test jest __tests__/integration --forceExit",
    "test:onboarding": "NODE_ENV=test jest __tests__/e2e/onboarding-flow.test.js __tests__/endpoints/organizaciones.test.js --forceExit",
    "test:file": "NODE_ENV=test jest --forceExit",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "docs": "node scripts/generate-docs.js"
  },
  "keywords": [
    "saas",
    "agendamiento",
    "multi-tenant",
    "api",
    "postgresql"
  ],
  "author": "Equipo SaaS",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "file-type": "^16.5.4",
    "minio": "^7.1.3",
    "pdfkit": "^0.15.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "mercadopago": "^2.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.10",
    "pg": "^8.11.3",
    "redis": "^5.8.2",
    "sharp": "^0.33.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
