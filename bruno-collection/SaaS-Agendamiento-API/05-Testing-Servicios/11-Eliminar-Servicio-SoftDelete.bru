meta {
  name: 11-Eliminar-Servicio-SoftDelete
  type: http
  seq: 11
}

delete {
  url: {{baseUrl}}/api/{{apiVersion}}/servicios/{{servicioBarbaId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

assert {
  res.status: eq 200
  res.body.success: eq true
  res.body.message: contains "eliminado exitosamente"
}

script:post-response {
  if (res.body.success) {
    console.log("âœ… Servicio eliminado (soft delete) exitosamente");
    console.log("Servicio ID eliminado:", "{{servicioBarbaId}}");
    console.log("Mensaje:", res.body.message);
  }
}